	no-sdio;                             // 禁用 SDIO 模式
	no-sd;                               // 禁用 SD 卡模式
	supports-emmc;                       // 支持 eMMC 设备
	non-removable;                       // 不可移除（eMMC 焊接在板上）
	mmc-hs200-1_8v;                      // 支持 HS200 模式，1.8V 供电
	pinctrl-names = "default";
	// 引用 eMMC 相关引脚配置（8 位总线、时钟、命令、数据选通）

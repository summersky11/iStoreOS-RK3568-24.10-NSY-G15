	status = "okay";                     // 启用 eMMC 控制器
};

// sdmmc2 控制器（SDIO 控制器，地址 fe000000，当前禁用）
&sdmmc2 {
	max-frequency = <150000000>;         // 最大时钟频率 150MHz
	fifo-depth = <256>;                  // FIFO 深度 256 字节
	no-sd;                               // 禁用 SD 卡模式
	no-mmc;                              // 禁用 MMC 模式
	bus-width = <4>;                     // 总线宽度 4 位
	disable-wp;                          // 禁用写保护
	cap-sd-highspeed;                    // 支持 SD 高速模式
	cap-sdio-irq;                        // 支持 SDIO 中断
	keep-power-in-suspend;               // 休眠时保持电源
	sd-uhs-sdr104;                       // 支持 UHS SDR104 模式
	mmc-pwrseq = <&sdio_pwrseq>;         // 关联 SDIO 电源序列控制器
	non-removable;                       // 不可移除（WiFi/蓝牙模块）
	pinctrl-names = "default";
